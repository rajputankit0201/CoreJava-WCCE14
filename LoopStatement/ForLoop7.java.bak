class ForLoop7
{
	public static void main(String[] args) 
	{
		for (int r =1;r<=5 ;r++ )
		{
			for (int j = 1;j<=5; j++ )
				{
				if (r>=3 && j>=3){
					System.out.print(" ");
				}
				else if ((r==5 && j == 2)||(r == 2 && j == 5))
				{
                    System.out.print("*");
				}
				else
                    System.out.print("*");
				}
					System.out.println();
			}
		}
	}