class Human1 
{
	//Method overloading
	public static void anger()
	{ 
		System.out.println("Anger is a nature of human");
	}
	//Constructor overloading
	public static void anger(String s);
	{
		System.out.println("Human gets angry bcz of" +s " reason");
	}
	public static void main(String[] args) 
	{
		System.out.println("Hello World!");
	}
